{
	"name": "travail",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "vitest run",
		"prepare": "husky install",
		"format": "biome format --write ./src",
		"lint": "biome lint --apply ./src",
		"db:setup": "npm run db:push && node ./src/lib/utils/setupdb.js",
		"db:studio": "npx drizzle-kit studio",
		"db:push": "drizzle-kit push:sqlite",
		"db:generate": "drizzle-kit generate:sqlite"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.5.3",
		"@iconify-json/mdi": "^1.1.53",
		"@iconify-json/svg-spinners": "^1.1.1",
		"@julr/unocss-preset-forms": "^0.1.0",
		"@melt-ui/pp": "^0.3.0",
		"@melt-ui/svelte": "^0.71.1",
		"@sveltejs/adapter-node": "^4.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@unocss/extractor-svelte": "^0.58.0",
		"drizzle-kit": "^0.20.7",
		"husky": "^9.0.10",
		"sharp": "^0.33.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-sequential-preprocessor": "^2.0.1",
		"typescript": "^5.0.0",
		"unocss": "^0.58.0",
		"vite": "^5.0.3",
		"vitest": "^1.0.0"
	},
	"type": "module",
	"dependencies": {
		"argon2": "^0.31.2",
		"better-sqlite3": "^9.2.2",
		"chart.js": "^4.3.0",
		"dotenv": "^16.3.1",
		"drizzle-orm": "^0.29.1",
		"jsonwebtoken": "^9.0.1",
		"nanoid": "^5.0.4",
		"svelte-sonner": "^0.3.11",
		"zod": "^3.21.4"
	}
}
