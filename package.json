{
  "name": "travail",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "vitest run",
    "prepare": "husky",
    "format": "biome format --write ./src",
    "lint": "biome lint --apply ./src",
    "db:setup": "npm run db:push && node ./src/lib/utils/setupdb.js",
    "db:studio": "npx drizzle-kit studio",
    "db:push": "drizzle-kit push:sqlite",
    "db:generate": "drizzle-kit generate:sqlite"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.2",
    "@iconify-json/mdi": "^1.2.0",
    "@iconify-json/svg-spinners": "^1.2.0",
    "@julr/unocss-preset-forms": "^0.1.0",
    "@melt-ui/pp": "^0.3.2",
    "@melt-ui/svelte": "^0.83.0",
    "@sveltejs/adapter-node": "^5.2.4",
    "@sveltejs/kit": "^2.5.28",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@unocss/extractor-svelte": "^0.62.4",
    "drizzle-kit": "^0.24.2",
    "husky": "^9.1.6",
    "svelte": "^4.2.19",
    "svelte-check": "^4.0.2",
    "svelte-sequential-preprocessor": "^2.0.1",
    "typescript": "^5.6.2",
    "unocss": "^0.62.4",
    "vite": "^5.4.7",
    "vitest": "^2.1.1"
  },
  "type": "module",
  "dependencies": {
    "argon2": "^0.41.1",
    "better-sqlite3": "^11.3.0",
    "chart.js": "^4.4.4",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.33.0",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.7",
    "sharp": "^0.33.5",
    "svelte-sonner": "^0.3.28",
    "zod": "^3.23.8"
  }
}
